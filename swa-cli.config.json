{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "template-doctor": {
      "appLocation": "packages/app",
      "apiLocation": "packages/api",
      "outputLocation": "dist",
      "appBuildCommand": "npm ci && npm run build",
      "apiBuildCommand": "rm -rf node_modules && npm ci --omit=dev",
      "apiLanguage": "node",
      "apiVersion": "18",
      "appName": "template-doctor-new",
      "resourceGroup": "template-doctor-rg"
    }
  }
}