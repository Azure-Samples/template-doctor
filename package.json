{
    "name": "template-doctor",
    "version": "0.0.1",
    "description": "An Azure Developer CLI template analysis and healing agent",
    "type": "module",
    "main": "bin/cli.js",
    "bin": {
        "template-doctor": "dist/cli.js"
    },
    "scripts": {
        "build": "tsc && npm run copy-assets",
        "start": "node --experimental-json-modules dist/cli.js",
        "copy-assets": "mkdir -p dist/templates dist/assets && cp -r src/templates/* dist/templates/ && cp -r assets/* dist/assets/"
    },
    "keywords": [
        "azd",
        "azure",
        "templates"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@types/express": "^5.0.3",
        "@types/node": "^24.0.14",
        "@types/serve-static": "^1.15.8",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "dotenv": "^17.2.0",
        "express": "^5.1.0",
        "open": "^10.2.0",
        "serve-static": "^2.2.0"
    }
}
