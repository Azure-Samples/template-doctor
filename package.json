{
    "name": "template-doctor",
    "version": "0.0.1",
    "description": "An Azure Developer CLI template analysis and healing agent",
    "type": "module",
    "main": "bin/cli.js",
    "bin": {
        "template-doctor": "dist/cli.js"
    },
    "scripts": {
        "dev": "tsx watch src/cli.ts",
        "build": "tsc && npm run copy-assets",
        "start": "node --experimental-json-modules -r tsconfig-paths/register dist/cli.js",
        "copy-assets": "mkdir -p dist/templates dist/assets && cp -r src/templates/* dist/templates/ && cp -r assets/* dist/assets/",
        "test-dashboard": "node --experimental-json-modules dist/testDashboard.js",
        "test-index": "node --experimental-json-modules dist/testDashboard.js --index",
        "demo-analyze": "node dist/cli.js analyze --repo=https://github.com/Azure-Samples/azd-templates",
        "demo-list": "node dist/cli.js list --serve",
        "demo-full": "node dist/demoTemplateList.js",
        "analyze-todo": "node dist/cli.js analyze --repo=https://github.com/anfibiacreativa/todo-nodejs-mongo-coreconf --open-dashboard",
        "link-global": "npm link"
    },
    "keywords": [
        "azd",
        "azure",
        "templates"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@types/express": "^5.0.3",
        "@types/node": "^24.0.14",
        "@types/serve-static": "^1.15.8",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.20.3",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "@octokit/graphql": "^9.0.1",
        "@octokit/rest": "^22.0.0",
        "dotenv": "^17.2.0",
        "express": "^5.1.0",
        "open": "^10.2.0",
        "serve-static": "^2.2.0"
    }
}
