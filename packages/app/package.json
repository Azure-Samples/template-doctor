{
  "name": "template-doctor-frontend",
  "version": "0.0.1",
  "description": "Template Doctor frontend",
  "scripts": {
    "test": "playwright test",
    "test:headless": "PLAYWRIGHT_HEADLESS=1 playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "browsers:install": "playwright install chromium",
    "start": "python3 -m http.server 8080",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "node build.js",
    "build:dev": "MODE=development node build.js",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "terser": "^5.32.0",
    "clean-css": "^5.3.3",
    "html-minifier-terser": "^7.2.0"
  }
}
